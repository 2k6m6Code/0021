        {
           if ( !isValidIP(myform.telnet_dstip.value) )
           {
              alert('IP format error !!!');
              return;
           }
        }

        function go()
        { 
        	var privilege=getcookie('privilege');
        	    if(privilege!=1) {alert('You do not have Privilege to do it'); return;} 
