   /usr/local/apache/qb/setuid/do_qbdynroute.pl &
   
   if [ "$dynroute_proto" = "OSPF" ]; then
   /sbin/service ripd stop
   /sbin/service zebra restart
   /sbin/service ospfd restart
   else
   /sbin/service ospfd stop
   /sbin/service zebra restart
   /sbin/service ripd restart
   fi
