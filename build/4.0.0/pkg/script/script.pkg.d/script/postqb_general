httpsport_org=`grep Listen $SSLCFG|awk '{print $2}'`
if [ "$httpsport_new" != "$httpsport_org" ] && [ "$httpsport_new" != "" ]
then
    sed -i -e '{ s/'$httpsport_org'/'$httpsport_new'/g}' $SSLCFG
    service httpd restart &                                   
fi
