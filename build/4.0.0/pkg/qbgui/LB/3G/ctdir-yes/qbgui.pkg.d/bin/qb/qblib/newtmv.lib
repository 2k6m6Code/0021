        my @tables=maintainRtable( action=>'GETPOOLISP', table_num=>$tmpalltmv );
        $alltmv=$alltmv."Pool:".$tmpalltmv.",";
        $alltmvname=$alltmvname."Pool:".$tmpalltmv.",";
        foreach my $test (@tables)
        {
            $alltmv=$alltmv.','.$test.',';
