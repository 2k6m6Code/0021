sub scriptEditDos
{
    print << "scriptEditDos";

   <script type="text/javascript" src="grid.js"></script>
   <script language="javascript">

    var myform;
	
    function cgi_dep_onload() { myform=window.document.forms[0]; checkState(); showclick();}

	function newCancel() { window.top.mainFrame.location.href="./dos.cgi"; }

	function setEnable(checkboxname,boolvalue)
	{
		var enable=document.getElementById(checkboxname);
		
		var enable_AN = document.getElementById('enable_AN');
		var enable_SR = document.getElementById('enable_SR');
		var enable_SF = document.getElementById('enable_SF');
		var enable_NF = document.getElementById('enable_NF');
		var enable_AA = document.getElementById('enable_AA');
		var enable_NN = document.getElementById('enable_NN');
		var enable_XM = document.getElementById('enable_XM');
		
		//alert(boolvalue);
		if(enable.checked==true)
		{
			
			enable_AN.disabled=false;
			enable_SR.disabled=false;
			enable_SF.disabled=false;
			enable_NF.disabled=false;
			enable_AA.disabled=false;
			enable_NN.disabled=false;
			enable_XM.disabled=false;
		}
		else
		{
			
			enable_AN.disabled=true;
			enable_SR.disabled=true;
			enable_SF.disabled=true;
			enable_NF.disabled=true;
			enable_AA.disabled=true;
			enable_NN.disabled=true;
			enable_XM.disabled=true;
		}
	}
	
	function setLog(checkboxname,boolvalue)
	{
		var logset=document.getElementById(checkboxname);
		var timenumber=document.getElementById('timenumber');
		var time=document.getElementById('time');
		var logprefix=document.getElementById('logprefix');
		if(logset.checked==true)
		{
			timenumber.disabled=false;
			time.disabled=false;
			logprefix.disabled=false;
		}
		else
		{
			timenumber.disabled=true;
			time.disabled=true;
			logprefix.disabled=true;
		}
	}
	
	function mytest(dosenable,dosenable_AN,dosenable_SR,dosenable_SF,dosenable_NF,dosenable_AA,dosenable_NN,dosenable_XM,doslogset,dostimenumber,dostime,doslogprefix)
	{
		var enable=document.getElementById('enable');
		
		var enable_AN = document.getElementById('enable_AN');
		var enable_SR = document.getElementById('enable_SR');
		var enable_SF = document.getElementById('enable_SF');
		var enable_NF = document.getElementById('enable_NF');
		var enable_AA = document.getElementById('enable_AA');
		var enable_NN = document.getElementById('enable_NN');
		var enable_XM = document.getElementById('enable_XM');
		
		var logset=document.getElementById('logset');
		var timenumber=document.getElementById('timenumber');
		var time=document.getElementById('time');
		var logprefix=document.getElementById('logprefix');
		if(dosenable=='true')
		{
			enable.checked=true;
		}
		if(dosenable=='false')
		{
			enable.checked=false;
		}
		setEnable('enable',dosenable);
		enable_AN.checked=dosenable_AN;
		enable_SR.checked=dosenable_SR;
		enable_SF.checked=dosenable_SF;
		enable_NF.checked=dosenable_NF;
		enable_AA.checked=dosenable_AA;
		enable_NN.checked=dosenable_NN;
		enable_XM.checked=dosenable_XM;
		
		if(doslogset=='true'){logset.checked=true;}
		if(doslogset=='false'){logset.checked=false;}
		setLog('logset',doslogset);
		timenumber.value=dostimenumber;
		time.value=dostime;
		logprefix.value=doslogprefix;
		//alert(dosenable_AN+'.'+dosenable_SR+'.'+dosenable_SF+'.'+dosenable_NF+'.'+dosenable_AA+'.'+dosenable_NN+'.'+dosenable_XM);
	}
	
	function SaveCOD() 
    {
		var type=document.getElementById('type').value;
        var enable=document.getElementById('enable').checked;
		
		var enable_AN = document.getElementById('enable_AN').checked;
		var enable_SR = document.getElementById('enable_SR').checked;
		var enable_SF = document.getElementById('enable_SF').checked;
		var enable_NF = document.getElementById('enable_NF').checked;
		var enable_AA = document.getElementById('enable_AA').checked;
		var enable_NN = document.getElementById('enable_NN').checked;
		var enable_XM = document.getElementById('enable_XM').checked;
		
		var logset=document.getElementById('logset').checked;
		var timenumber=document.getElementById('timenumber').value;
		var time=document.getElementById('time').value;
		var logprefix=document.getElementById('logprefix').value;
        
		var n_dosip=document.getElementById('dosip');
		var dosip=n_dosip.value;
        var description=document.getElementById('description').value;

		if ( dosip == '' )
        {
            alert('IP must be English or nember');
            return;
        }

		//for (i = 0; i < schname.length; i++)
        //{
        //    var ch = schname.charCodeAt(i);
        //    if ( ch < 32 || ch > 126 )
        //    {
        //       alert("Schedule Name must be English or nember");
        //       return;
        //    }
        //}

		for (i = 0; i < description.length; i++)
        {
            var ch = description.charCodeAt(i);
            if ( ch < 32 || ch > 126 )
            {
               alert("Description must be English ");
               return;
            }
        }
        if ( n_dosip.disabled != true )
            myform.dosip.value = n_dosip.value;
        
//        myform.action.value = 'SAVE';
		myform.newtype.value=type;
		myform.newenable.value=enable;
		
		myform.newAN.value=enable_AN;
		myform.newSR.value=enable_SR;
		myform.newSF.value=enable_SF;
		myform.newNF.value=enable_NF;
		myform.newAA.value=enable_AA;
		myform.newNN.value=enable_NN;
		myform.newXM.value=enable_XM;
		
		myform.newlogset.value=logset;
		myform.newtimenumber.value=timenumber;
		myform.newtime.value=time;
		myform.newlogprefix.value=logprefix;
		//alert(enable_AN);
//        myform.submit();
	goSubmit('SAVE');
    }
	
	function checkState()
    {
        var action=document.getElementById('action').value;
        var dosip=document.getElementById('dosip');
        var dosip_hidden=document.getElementById('dosip_hidden');
        if ( dosip.value != '' ){
            dosip_hidden.disabled = true; }
        
        if ( action != '' ){
            //window.top.mainFrame.location.href="newportscan.cgi";
			history.go(-2); 
			location.replace('newportscan.cgi');
		}
    }

	function showclick() 
	{
		document.getElementById('AutoSubmit').click();
	}
	
	function select_type() 
	{
		var type=document.getElementById('type');
		var dosip=document.getElementById('dosip');
		
		if (type.selectedIndex != -1)
		{
			dosip.value = type.options[type.selectedIndex].text;
			if(type.options[type.selectedIndex].text=='Custom')
			{
				dosip.value = '';
				dosip.readonly=true;
			}else
			{
				dosip.value = type.options[type.selectedIndex].text;
				dosip.readonly = false;
			}
			
			
		}
	}
	
    </script>
    
scriptEditDos
}
