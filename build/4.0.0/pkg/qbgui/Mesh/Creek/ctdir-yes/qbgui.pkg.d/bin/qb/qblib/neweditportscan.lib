sub maintainCOD
{
    my (%action)=@_;

    my $dosref=XMLread($gPATH.'newportscan.xml');
    my $doslist=$dosref->{newportscan};
    my $exist=0;
	my $exist2=0;
    my %newportscan;
	my %newportscans;
	
	my $dqos=XMLread('/usr/local/apache/qbconf/dqos.xml');
    my $hostList=$dqos->{list};
	
	@type=$action{newtype};
	@enable=$action{newenable};
	
	@enable_AN=$action{newAN};
	@enable_SR=$action{newSR};
	@enable_SF=$action{newSF};
	@enable_NF=$action{newNF};
	@enable_AA=$action{newAA};
	@enable_NN=$action{newNN};
	@enable_XM=$action{newXM};
	
	@logset=$action{newlogset};
	@timenumber=$action{newtimenumber};
	@time=$action{newtime};
	@logprefix=$action{newlogprefix};
    
    $action{dosip}=~s/^\s*|\s*$//g;
    if ( !$action{action} ) { return; }
    
    if ( $action{action}=~m/^SAVE$/ )
    {
		@type=$action{newtype};
		@enable=$action{newenable};
		@enable_AN=$action{newAN};
		@enable_SR=$action{newSR};
		@enable_SF=$action{newSF};
		@enable_NF=$action{newNF};
		@enable_AA=$action{newAA};
		@enable_NN=$action{newNN};
		@enable_XM=$action{newXM};
		@logset=$action{newlogset};
		@timenumber=$action{newtimenumber};
		@time=$action{newtime};
		@logprefix=$action{newlogprefix};
		
		$newportscan{type}=$type[0];
        $newportscan{dosip}=$action{dosip};
        $newportscan{description}=$action{description};
		$newportscan{enable}=$enable[0];
		
		$newportscan{enable_AN}=$enable_AN[0];
		$newportscan{enable_SR}=$enable_SR[0];
		$newportscan{enable_SF}=$enable_SF[0];
		$newportscan{enable_NF}=$enable_NF[0];
		$newportscan{enable_AA}=$enable_AA[0];
		$newportscan{enable_NN}=$enable_NN[0];
		$newportscan{enable_XM}=$enable_XM[0];
		
		$newportscan{logset}=$logset[0];
		$newportscan{timenumber}=$timenumber[0];
		$newportscan{time}=$time[0];
		$newportscan{logprefix}=$logprefix[0];
		
		$newportscans{list}=$action{dosip};
		
        foreach my $sh ( @$doslist )
        {
            if ( $sh->{dosip} eq $action{dosip} )
            {
                $exist=1;
                $sh=\%newportscan;
            }
        }
        if ( $exist eq '0' )
        {
            #delete($action{action});
            push( @$doslist, \%newportscan); 
            @$doslist=sort byName @$doslist;
        }
	foreach my $sh ( @$hostList )
	{
		if ( $sh->{list} eq $action{dosip} )
		{
			$exist2=1;
			$hostList=\%newportscans;
		}
	}
	if ( $exist2 eq '0' ){push( @$hostList, \%newportscans);}
        LogUserAction( action=>'SAVECOD', dosip=>$action{dosip});
    }
    elsif ( $action{action}=~m/^DELET$/ )
    {
        my $delet=$action{newportscan};
        if( @$delet <= 0 )
        {
            $gMSGPROMPT.=qq (Please select some SUBNET first\\n\\n);
            return;
        }
        
        my @temparray;
		my @temparray2;
        foreach my $item ( @$doslist )
        {
            if ( grep(/^$item->{dosip}$/, @$delet) ) { LogUserAction( action=>'DELCOD', dosip=>$item->{dosip} ); next; }
            push(@temparray, $item); 
        }
		foreach my $item ( @$hostList )
        {
            if ( grep(/^$item->{list}$/, @$delet) ) { LogUserAction( action=>'DELCOD', list=>$item->{list} ); next; }
            push(@temparray2, $item); 
        }
        $dosref->{newportscan}=\@temparray;
		$dqos->{list}=\@temparray2;
        
    }
	elsif ( $action{action}=~m/^ENABL$/ )
    {
		my $enable=$action{enable};
		my @temparray;
        foreach my $item ( @$doslist )
        {
		
		if ( grep(/^$item->{dosip}$/, @$enable) )
            {
if ( $item->{dosip} eq "system" ) { next; }
                $item->{enable}=true;
		$item->{logset}=true;
            }
		else{$item->{enable}=false; $item->{logset}=false;}
            push(@temparray, $item); 
        }
        $dosref->{newportscan}=\@temparray;
    }
         XMLwrite($dqos, $gPATH."dqos.xml");
    XMLwrite($dosref, $gPATH."newportscan.xml");
    #makeshell();
}

