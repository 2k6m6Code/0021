        {
            var ch = schname.charCodeAt(i);
            if ( ch < 32 || ch > 126 )
            {
               alert("Schedule Name must be English or nember");
               return;
            }
        }

	for (i = 0; i < description.length; i++)
        {
            var ch = description.charCodeAt(i);
            if ( ch < 32 || ch > 126 )
            {
               alert("Schedule Description must be English ");
               return;
            }
        }
        
